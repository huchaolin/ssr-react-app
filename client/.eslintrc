{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "airbnb",
    "rules": {
        "semi": [0],
        "no-extra-semi": [0],
        "react/jsx-filename-extension": [0],
        "react/jsx-indent": [0],
        "react/destructuring-assignment": [0],
        "no-useless-constructor": [0],
        "react/no-typos": [0],
        "no-console": [0],
        "react/require-default-props": [0],
        "react/prefer-stateless-function": [0],
        "no-unused-vars": [0],
        "indent": [0],
        "max-len": [0]
    }
}
